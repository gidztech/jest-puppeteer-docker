{
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "tabWidth": 4
            }
        ],
        "no-console": "off"
    },
    "env": {
        "node": true,
        "jest": true,
        "es6": true
    },
    "globals": {
        "page": true,
        "extensions": true,
        "runSetup": true,
        "visualCheck": true
    },
    "parserOptions": {
        "ecmaVersion": 8
    }
}
